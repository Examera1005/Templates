{
  "name": "react-native-mobile-app-template",
  "version": "1.0.0",
  "description": "A comprehensive React Native mobile app template with authentication, navigation, and modern features",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "cd ios && xcodebuild -workspace *.xcworkspace -scheme * -configuration Release archive && cd ..",
    "postinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.4",
    
    "// Navigation": "",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "react-native-screens": "^3.22.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.4.2",
    
    "// Storage & Security": "",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "react-native-keychain": "^8.1.3",
    "react-native-encrypted-storage": "^4.0.3",
    
    "// Network & API": "",
    "axios": "^1.4.0",
    "@react-native-community/netinfo": "^9.4.1",
    
    "// Authentication & Biometrics": "",
    "react-native-biometrics": "^3.0.1",
    "react-native-touch-id": "^4.4.1",
    
    "// Notifications": "",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "react-native-push-notification": "^8.1.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    
    "// Media & Camera": "",
    "react-native-image-picker": "^5.6.0",
    "react-native-permissions": "^3.8.4",
    
    "// Utilities": "",
    "react-native-device-info": "^10.8.0",
    "react-native-uuid": "^2.0.1",
    "date-fns": "^2.30.0",
    
    "// State Management (Optional)": "",
    "zustand": "^4.4.1",
    
    "// Development": "",
    "react-native-flipper": "^0.212.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "keywords": [
    "react-native",
    "mobile",
    "template",
    "authentication",
    "navigation",
    "biometrics",
    "push-notifications",
    "offline-storage",
    "theming"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-native-mobile-app-template.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-native-mobile-app-template/issues"
  },
  "homepage": "https://github.com/yourusername/react-native-mobile-app-template#readme"
}